AK,EAAC,2IAGZ;AAFM,IAAA,IAAI,EAAC,MAEX;;aADe,KAAA,CAAA,IAAA,CAAK,aAAL,GAAkB,M;;AACjC,GAHD,E,IAAA,E;;AAAA,G,iBAEgB,KAAA,CAAA,IAAA,CAAK,a,GAEnB,CARN,CAYE,EAHF,YAAA,CAEM,KAFN,EAAA,UAAA,EAEM,CADF,YAAA,CAAmJ,QAAnJ,EAAmJ;AAA3I,IAAA,KAAK,EAAC,gFAAqI;AAApD,IAAA,IAAI,EAAC,QAA+C;AAAjC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,MAAA,EAAF;AAAA,KAAA;AAA4B,GAAnJ,EAAmI,SAAnI,CACE,CAFN,CAGE,CAbN,CAcE,CAfN,CAgBG,CAjBP,C","sourcesContent":["<template>\n    <form class=\"container w-auto\">\n        <div class=\"my-12 md:mx-6 sm:mx-6 xl:mx-56 lg:mx-56\">\n            <div class=\"flex flex-wrap\">\n                <div class=\"w-full px-3 mb-6 md:mb-4\">\n                    <label class=\"block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2\">\n                        Libell√© Niveau\n                    </label>\n                    <input class=\"mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-300 focus:ring focus:ring-indigo-200 focus:ring-opacity-50 p-2\"\n                           type=\"text\"\n                           v-model=\"form.libelleNiveau\"\n                    >\n                </div>\n                <div class=\"w-full px-3 mt-12\">\n                    <button class=\"bg-red-500 hover:bg-red-700 text-white py-2 px-4 rounded font-semibold text-sm\" type=\"button\" v-on:click=\"submit()\">ENVOYER</button>\n                </div>\n            </div>\n        </div>\n    </form>\n</template>\n\n<script>\n    import http from \"../../http-common\"\n\n    export default {\n        name: \"NiveauUpdate\",\n        data() {\n            return {\n                form: {\n                    libelleNiveau: ''\n                }\n            }\n        },\n        mounted() {\n            this.getNiveau()\n        },\n        methods: {\n            submit: function (id) {\n                id = this.$route.params.id;\n\n                http\n                    .put( 'niveaux/' + id, this.form)\n                    .then(function( response ){\n                        this.stat = response.status\n                        if (this.stat === 200) {\n                            this.$toast.success(`Niveau resource updated`, {\n                                position: \"top-right\"\n                            })\n                            setTimeout(this.$toast.clear, 3500)\n                            this.$router.push({ name: 'Niveau' })\n                        }\n                        else if (this.stat === 400) {\n                            this.$toast.error(`Invalid input`, {\n                                position: \"top-right\"\n                            })\n                        }\n                        else if (this.stat === 404) {\n                            this.$toast.error(`Resource not found`, {\n                                position: \"top-right\"\n                            })\n                        }\n                        else if (this.stat === 422) {\n                            this.$toast.error(`Unprocessable entity`, {\n                                position: \"top-right\"\n                            })\n                        }\n                    }.bind(this))\n            },\n            getNiveau: function (id) {\n                const self = this;\n                id = this.$route.params.id;\n\n                http\n                    .get('niveaux/' + id)\n                    .then(function (response) {\n                        self.form = response.data\n                    })\n            }\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":""}]}