     this.getMention()\n        },\n        methods: {\n            getMention: function () {\n                axios\n                    .get(BaseUrl + 'mentions')\n                    .then(res => (this.mention = res.data['hydra:member']))\n            },\n            deleteMention: function (id) {\n                axios\n                    .delete(BaseUrl + 'mentions/' + id)\n                    .then(() => { this.getMention() })\n            },\n            modifMention: function () {\n\n            }\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":""}]}