lock w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-300 focus:ring focus:ring-indigo-200 focus:ring-opacity-50 p-2\"\n                           type=\"text\"\n                           v-model=\"form.libelleInstitut\"\n                           required\n                    >\n                </div>\n                <div class=\"w-full px-3 mt-12\">\n                    <button class=\"bg-red-500 hover:bg-red-700 text-white py-2 px-4 rounded font-semibold text-sm\" type=\"button\" v-on:click=\"submit()\">MODIFIER</button>\n                </div>\n            </div>\n        </div>\n    </form>\n</template>\n\n<script>\n    import http from \"../../http-common\"\n\n    export default {\n        name: \"ComposanteUpdate\",\n        data() {\n            return {\n                form: {\n                    libelleInstitut: '',\n                },\n            }\n        },\n        mounted() {\n            this.getComposante();\n        },\n        methods: {\n            getComposante: function (id) {\n                id = this.$route.params.id\n                const self = this;\n\n                http\n                    .get('composantes/' + id)\n                    .then(res => {\n                        self.form = res.data\n                    })\n            },\n            submit: function (id) {\n                id = this.$route.params.id\n\n                http\n                    .put( 'composantes/' + id, this.form)\n                    .then(function( response ){\n                        this.stat = response.status\n                        if (this.stat === 200) {\n                            this.$toast.success(`Composante resource updated`, {\n                                position: \"top-right\"\n                            })\n                            setTimeout(this.$toast.clear, 3500)\n                            this.$router.push({ name: 'Composante' })\n                        }\n                        else if (this.stat === 400) {\n                            this.$toast.error(`Invalid input`, {\n                                position: \"top-right\"\n                            })\n                        }\n                        else if (this.stat === 404) {\n                            this.$toast.error(`Resource not found`, {\n                                position: \"top-right\"\n                            })\n                        }\n                        else if (this.stat === 422) {\n                            this.$toast.error(`Unprocessable entity`, {\n                                position: \"top-right\"\n                            })\n                        }\n                    }.bind(this))\n            }\n        }\n    }\n</script>\n\n<style scoped>\n</style>"]}]}