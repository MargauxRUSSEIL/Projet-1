t class=\"mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-300 focus:ring focus:ring-indigo-200 focus:ring-opacity-50 p-2\"\n                                   type=\"search\"\n                                   placeholder=\"Rechercher\"\n                                   v-model=\"searchNiveau\"\n                            >\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"shadow overflow-hidden border-b border-gray-200 sm:rounded-lg\">\n                <table class=\"w-full table-auto divide-y divide-gray-200\">\n                    <thead class=\"bg-gray-50\">\n                    <tr>\n                        <th scope=\"col\" class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">libellé niveau</th>\n                        <th scope=\"col\" class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"></th>\n                    </tr>\n                    </thead>\n                    <tbody class=\"bg-white divide-y divide-gray-200\">\n                    <tr class=\"hover:bg-gray-100\" v-for=\"item in filtered\" :key=\"item\">\n                        <td class=\"px-6 py-4 whitespace-nowrap\">{{ item.libelleNiveau }}</td>\n                        <td class=\"px-6 py-4 whitespace-nowrap text-right text-sm font-medium\">\n                            <div class=\"text-sm text-gray-900\">\n                                <router-link :to=\"{ name: 'updateNiveau', params: { id: item.id }}\">\n                                    <button class=\"text-indigo-600 hover:text-indigo-900 font-semibold\">Modifier</button>\n                                </router-link>\n                            </div>\n                            <div class=\"text-sm text-gray-900\">\n                                <button class=\"text-indigo-600 hover:text-indigo-900 font-semibold\" v-on:click=\"deleteNiveau(item.id)\">Supprimer</button>\n                            </div>\n                        </td>\n                    </tr>\n                    </tbody>\n                </table>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n  import http from \"../../http-common\"\n\n    export default {\n        name: \"NiveauTable\",\n        data () {\n            return {\n                searchNiveau: '',\n                stat: '',\n                errored: false,\n                niveau: []\n            }\n        },\n        mounted() {\n            this.getNiveau()\n        },\n        methods: {\n            getNiveau: function () {\n                http\n                    .get('niveaux')\n                    .then(res => {\n                        this.niveau = res.data['hydra:member']\n                        const total = res.data['hydra:totalItems']\n                        if (total === 0) {\n                            this.errored = true\n                        }\n                    })\n            },\n            deleteNiveau: function (id) {\n                http\n                    .delete('niveaux/' + id)\n                    .then(function( response ){\n                        this.stat = response.status\n                        if (this.stat === 204) {\n                            this.getNiveau()\n                            this.$toast.success(`Niveau resource deleted`, {\n                                position: \"top-right\"\n                            })\n                            setTimeout(this.$toast.clear, 3500)\n                        }\n                        else if (this.stat === 404) {\n                            this.$toast.error(`Resource not found`, {\n                                position: \"top-right\"\n                            })\n                        }\n                    }.bind(this))\n            }\n        },\n        computed: {\n            filtered: function () {\n                let search = this.niveau;\n                const searchNiveau = this.searchNiveau;\n\n                if (!searchNiveau) {\n                    return search;\n                }\n                search = search.filter(function (item) {\n                    if (item.libelleNiveau.toLowerCase().indexOf(searchNiveau) !== -1 || item.libelleNiveau.toUpperCase().indexOf(searchNiveau) !== -1) {\n                        return item;\n                    }\n                })\n                return search;\n            }\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"]}]}