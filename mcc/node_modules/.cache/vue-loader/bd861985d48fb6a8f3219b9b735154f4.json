        <div class=\"flex flex-wrap\">\n                <div class=\"w-full px-3 mb-6 md:mb-4\">\n                    <label class=\"block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2\">\n                        Libellé du diplôme\n                    </label>\n                    <input class=\"mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-300 focus:ring focus:ring-indigo-200 focus:ring-opacity-50 p-2\"\n                           type=\"text\"\n                           v-model=\"form.libelleDiplome\"\n                    >\n                </div>\n                <div class=\"w-full px-3 mb-6 md:mb-4\">\n                    <label class=\"block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2\">\n                        Nombre année du diplôme\n                    </label>\n                    <input class=\"mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-300 focus:ring focus:ring-indigo-200 focus:ring-opacity-50 p-2\"\n                           type=\"number\"\n                           v-model=\"form.nbAnneeDiplome\"\n                    >\n                </div>\n                <div class=\"w-full px-3 mt-12\">\n                    <button class=\"bg-red-500 hover:bg-red-700 text-white py-2 px-4 rounded font-semibold text-sm\" type=\"button\" v-on:click=\"submit()\">ENVOYER</button>\n                </div>\n            </div>\n        </div>\n    </form>\n</template>\n\n<script>\n    import http from \"../../http-common\"\n\n    export default {\n        name: \"DiplomeEtablissementUpdate\",\n        data() {\n            return {\n                form: {\n                    libelleDiplome: '',\n                    nbAnneeDiplome: '',\n                }\n            }\n        },\n        mounted() {\n          this.getDiplome()\n        },\n        methods: {\n            submit: function (id) {\n                id = this.$route.params.id;\n\n                http\n                    .put( 'diplome_etablissements/' + id, this.form)\n                    .then(function( response ){\n                        this.stat = response.status\n                        if (this.stat === 200) {\n                            this.$toast.success(`Diplome resource updated`, {\n                                position: \"top-right\"\n                            })\n                            setTimeout(this.$toast.clear, 3500)\n                            this.$router.push({ name: 'Diplome' })\n                        }\n                        else if (this.stat === 400) {\n                            this.$toast.error(`Invalid input`, {\n                                position: \"top-right\"\n                            })\n                        }\n                        else if (this.stat === 404) {\n                            this.$toast.error(`Resource not found`, {\n                                position: \"top-right\"\n                            })\n                        }\n                        else if (this.stat === 422) {\n                            this.$toast.error(`Unprocessable entity`, {\n                                position: \"top-right\"\n                            })\n                        }\n                    }.bind(this))\n            },\n            getDiplome: function (id) {\n                const self = this;\n                id = this.$route.params.id;\n\n                http\n                    .get('diplome_etablissements/' + id)\n                    .then(function (response) {\n                        self.form = response.data\n                    })\n            }\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"]}]}